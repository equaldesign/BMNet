<cfcomponent name="general" output="false"  autowire="true">  <cfproperty name="QuoteService" inject="id:quote.QuoteService">	<cffunction name="preHandler">		<cfargument name="event">		<cfset var rc = event.getCollection()>		<cfif isUserInRole("staff")>		  <cfset rc.menuPath = "/merchant">		<cfelseif getAuthUser() neq "">			<cfset rc.menuPath = "/trade">		<cfelse>			<cfset rc.menuPath = "/">		</cfif>	</cffunction><!------------------------------------------- PUBLIC EVENTS ------------------------------------------>	<!--- Default Action --->	<cffunction name="index" returntype="void" output="false" hint="My main event">		<cfargument name="event" required="true">		<cfscript>		  var rc = event.getCollection();		  rc.fromDate = event.getValue("fromDate","");      rc.pendingQuotations = QuoteService.getPendingQuotations();      rc.openQuotations = QuoteService.getOpenQuotations();		  rc.toDate = event.getValue("toDate","");		  event.setView("home/#request.BMnet.company_type#/index");    	</cfscript>	</cffunction>  <cffunction name="pending" returntype="void" output="false" hint="My main event">    <cfargument name="event" required="true">    <cfscript>      var rc = event.getCollection();      rc.pendingQuotations = QuoteService.getPendingQuotations();      event.setView("quote/list/#request.BMnet.company_type#/pending");      </cfscript>  </cffunction>  <cffunction name="open" returntype="void" output="false" hint="My main event">    <cfargument name="event" required="true">    <cfscript>      var rc = event.getCollection();      rc.openQuotations = QuoteService.getOpenQuotations();      event.setView("quote/list/#request.BMnet.company_type#/open");      </cfscript>  </cffunction>  <cffunction name="rejected" returntype="void" output="false" hint="My main event">    <cfargument name="event" required="true">    <cfscript>      var rc = event.getCollection();      event.setView("quote/list/#request.BMnet.company_type#/rejected");      </cfscript>  </cffunction>  <cffunction name="success" returntype="void" output="false" hint="My main event">    <cfargument name="event" required="true">    <cfscript>      var rc = event.getCollection();      event.setView("quote/list/#request.BMnet.company_type#/success");      </cfscript>  </cffunction><!------------------------------------------- PRIVATE EVENTS ------------------------------------------></cfcomponent>