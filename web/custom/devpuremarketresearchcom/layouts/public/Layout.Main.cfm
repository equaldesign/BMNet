<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>	<meta charset="utf-8">	<title>Pure Market Research</title>  <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900' rel='stylesheet' type='text/css'>  <script language="javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.js"></script>  <link rel="shortcut icon" href="/favicon.ico" />	<link href="/includes/style/bootstrap.css" rel="stylesheet">	<link href="/includes/style/bootstrap-responsive.min.css" rel="stylesheet">  <link href="/includes/style/sites/11/main.css" rel="stylesheet"><cfoutput>  #getMyPlugin(plugin="jQuery").getDepends("prettify,bootstrap,bbq,address,form,block,cycle,easing.1.3,cookie","","jQuery/prettify",false)#</cfoutput></head><body>	<div class="navbar navbar-fixed-top">		<div class="navbar-inner">			<div class="container">				<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">          <span class="icon-bar"></span>          <span class="icon-bar"></span>          <span class="icon-bar"></span>        </a>				<a class="brand" href="#event.buildLink('')#"><strong><img src="/includes/images/sites/11/toplogo.png" border="0" /></strong></a>				<div class="nav-collapse collapse">          <ul class="nav">            <cfoutput>            <li class="#IIf(paramValue("rc.pageData.page.name","") eq "index","'active'","''")#">              <a href="/">Home</a>            </li>            <li class="#IIf(paramValue("rc.pageData.page.name","") eq "about","'active'","''")#">              <a href="/page/about">About</a>            </li>            <li class="#IIf(paramValue("rc.pageData.page.name","") eq "services","'active'","''")#">              <a href="/page/services">Services</a>            </li>            <li class="#IIf(paramValue("rc.pageData.page.name","") eq "data","'active'","''")#">              <a href="/page/data">Bid Request</a>            </li>            <li class="#IIf(paramValue("rc.pageData.page.name","") eq "contact","'active'","''")#">              <a href="/page/contact">Contact</a>            </li>            </cfoutput>          </ul>        </div>        <cfif getAuthUser() eq "">        <div class="pull-right">          <form class="form form-inline" action="/eunify/login/doLogin" method="post">            <input name="j_username" class="input-medium" type="text" placeholder="username..." />            <input name="j_password" class="input-medium" type="password" placeholder="password..." />            <input type="submit" class="btn btn-success" value="Login" />          </form>        </div>        <cfelse>          <cfset currentUser = getModel("eunify.ContactService").getContact(request.BMNet.contactID,3,request.siteID)>            <div id="loggedin">            <cfoutput>              <p><i class="icon-loggedin"></i>Logged in as #currentUser.first_name# #currentUser.surname# | <a href="/login/logout">Not you?</a></p>            </cfoutput>            </div>        </cfif>			</div>		</div>	</div>  <div class="container">    <cfoutput>#renderView()#</cfoutput>  </div>	<footer class="footer">	</footer><cfif isUserInAnyRole("staff,ebiz")>  <link type="text/css" rel="stylesheet" href="/includes/javascript/jQuery/css/Aristo/jQueryUI.css"></link>  <div id="dialog"></div>  <cfoutput>#renderView(view="admin/menu",module="sums")#</cfoutput></cfif></body></html>